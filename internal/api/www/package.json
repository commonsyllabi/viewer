{
  "name": "commonsyllabi-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --mode development --emptyOutDir",
    "watch": "vite build --watch --mode development --emptyOutDir",
    "preview": "vite preview",
    "test": "cypress run",
    "serve:test": "vite & cypress run --record false && pkill node"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "cypress": "^9.6.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.9"
  }
}
