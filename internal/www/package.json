{
  "name": "commonsyllabi-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --ext .js,.vue --ignore-path ../../.gitignore --fix src",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint:markup": "vue-tsc --noEmit",
    "format": "prettier .  --write",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "watch": "vite build --watch --mode development --emptyOutDir",
    "preview": "vite preview",
    "start": "yarn build & (cd ../../ && go run cmd/api/main.go)",
    "test": "cypress run",
    "autotest": "start-server-and-test 8080 'cypress run -q'"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix",
    "*.{css,scss,vue}": "stylelint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "bootstrap": "5.1.3",
    "bootstrap-vue": "^2.22.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "cypress": "^9.6.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "http-server": "^14.1.0",
    "prettier": "^2.6.2",
    "sass": "^1.50.1",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.8.1",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.9"
  }
}
