{
  "name": "commonsyllabi-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "watch": "vite build --watch --mode development --emptyOutDir",
    "preview": "vite preview",
    "start": "yarn build & http-server -c-1 --silent",
    "test": "cypress run",
    "autotest": "start-server-and-test 8080 'cypress run -q'"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "bootstrap-vue": "^2.22.0",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "cypress": "^9.6.0",
    "eslint-config-prettier": "^8.5.0",
    "http-server": "^14.1.0",
    "prettier": "^2.6.2",
    "sass": "^1.50.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.9"
  }
}
